            <material:DataGridColumn ValueBinding="{Binding CategoryName}" Title="Category"/>

            <material:DataGridColumn ValueBinding="{Binding ProductNumber}" Title="Product Number"/>

            <material:DataGridColumn ValueBinding="{Binding Names}" Title="Name" />
        </material:DataGrid.Columns>
    </material:DataGrid>-->
</uranium:UraniumContentPage>